@startuml
skinparam monochrome true
skinparam StateFontName Helvetica
skinparam defaultFontName Monospaced
skinparam defaultFontStyle Bold
skinparam state {
    FontColor Black
    FontStyle Bold
}
state TurnstileFSM {
LOCKED --> UNLOCKED : COIN
UNLOCKED --> UNLOCKED : COIN
UNLOCKED --> LOCKED : PASS
}
@enduml
